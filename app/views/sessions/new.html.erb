<h1>Login</h1>
<%= form_for(:session, url: "/login") do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.submit "Log In", class: "login-button2" %>
<% end %>

<p>Not a registered user? <%= link_to "Create an account!", new_user_path %></p>
